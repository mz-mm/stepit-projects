<Application x:Class="WarehouseMS.Presentation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WarehouseMS.Presentation"
             xmlns:View="clr-namespace:WarehouseMS.Presentation.Views"
             xmlns:ViewModel="clr-namespace:WarehouseMS.Presentation.ViewModels">
             
    <Application.Resources>
        <ResourceDictionary>

            <DataTemplate DataType="{x:Type ViewModel:MainViewModel}">
                <View:MainView/>
            </DataTemplate>

        </ResourceDictionary>
    </Application.Resources>
</Application>
